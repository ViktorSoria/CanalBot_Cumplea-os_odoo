<odoo>
    <data>

        <record model="ir.ui.view" id="wizard_download_data_form_view">
            <field name="name">wizard.download.data.form</field>
            <field name="model">wizard.download.data</field>
            <field name="arch" type="xml">
                <form string="Informacion a descargar">
                    <group>
<!--                        <field name="file_name" required="True"/>-->
                        <field name="location_id"
                               options="{'no_create': True, 'no_create_edit':True, 'no_quick_create': True, 'no_open': True}"
                               domain="[('usage', '=', 'internal')]"/>
                        <field name="stock_quants_ids" invisible="True"/>
                    </group>
                    <footer>
                        <button name="download_data" type="object" string="Descargar" class="oe_highlight"/>
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="wizard_warning_data">
            <field name="name">wizard.warning.data</field>
            <field name="model">wizard.warning</field>
            <field name="arch" type="xml">
                <form string="Warning">
                        <field name="msg" readonly="True"/>
                    <footer>
                        <button name="accept" type="object" string="Aceptar" class="oe_highlight"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_download_stockquant" model="ir.actions.server">
            <field name="name">Descargar excel</field>
            <field name="model_id" ref="stock_picking_custom.model_wizard_download_data"/>
            <field name="binding_model_id" ref="stock.model_stock_quant"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">
                    action = model.call_wizard_excel()
            </field>
        </record>

        <record id="act_download_data" model="ir.actions.act_window">
            <field name="name">Descargar Informaci√≥n</field>
            <field name="res_model">wizard.download.data</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="wizard_download_data_form_view"/>
            <field name="target">new</field>
        </record>

        <menuitem id="menu_download_data" parent="stock.menu_warehouse_report" name="Descargar Existencias" sequence="1000"
                  action="act_download_data" groups="stock.group_stock_manager,stock.group_stock_user"/>
    </data>
</odoo>